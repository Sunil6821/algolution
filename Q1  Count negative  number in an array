#include<bits/stdc++.h>
#include<unordered_map>

using namespace std;
 int countNegativeNO(vector<int> arr) {
     int count = 0;
     for (int num : arr) {
         if (num < 0) {
             count++;
         }
     }
     return count;
 }

 int main() {
     vector<int> arr = {-1, -15, -3, 3 -5};
     int result = countNegativeNO(arr);
     cout<<result;
     return 0;
 }
